{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
   "rules": {
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "version": "17"
    }
  }
}
