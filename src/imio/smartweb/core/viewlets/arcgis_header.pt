  <style>
    #viewDiv {
      top: 71px;
      height: calc(100% - 71px);
      width: 100%;
      padding: 0;
      margin: 0;
      position: absolute;
    }

    .navbar-viewer {
      height: 71px;
      width: 100%;
      padding: 0;
      margin: 0;
      position: absolute;
      background-image: url('../../../assets/img/navapp.png');
      background-size: cover;
      overflow: hidden;
    }

    .container-fluid{
      overflow: hidden;
    }

    .action-button {
      font-size: 16px;
      background-color: transparent;
      border: 1px solid #d3d3d3;
      color: #6e6e6e;
      height: 32px;
      width: 32px;
      text-align: center;
      box-shadow: 0 0 1px rgba(0, 0, 0, 0.3);
    }

    .action-button:hover,
    .action-button:focus {
      background: #0079c1;
      color: #e4e4e4;
    }

    .active {
      background: #0079c1;
      color: #e4e4e4;
    }

    #toolbar {
      height: 50px;
      width: 100%;
      padding: 0;
      margin: 0;
      display: flex;
      justify-content: end;
    }

    #backbar {
      position: fixed;
      height: 35px;
      width: 100%;
      padding: 0;
      margin: 0;
      display: flex;
      justify-content: center;
      bottom: 0;
    }

    #backbar a {
      padding: 0;
      margin: 0;
      color: #ffffff;
      text-decoration: none;
    }

    #linkBack {
      width: 250px;
      height: 100%;
      padding: 5px 20px;
      background-color: #006F7F;
      border-radius: 20px 20px 0 0;
    }

    #linkBack i {
      margin-right: 15px;
    }

    #disciplines-filter {
      height: 240px;
      width: 150px;
      visibility: hidden;
      overflow: scroll;
    }

    .discipline-item {
      width: 100%;
      padding: 12px;
      text-align: center;
      vertical-align: baseline;
      cursor: pointer;
      height: 40px;
      background-color: #ffffff;
    }

    .discipline-item:focus {
      background-color: dimgrey;
    }

    .discipline-item:hover {
      background-color: dimgrey;
    }

    #titleDiv {
      padding: 20px;
      background-color: #006F7F;
      color: #FFFFFF;
      border-radius: 5px;
    }

    #titleText {
      font-size: 20pt;
      font-weight: 100;
      padding-bottom: 10px;
    }
  </style>
